FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
  bottom
  {
      type            noSlip; 
  }
  top
  {
      type            noSlip;
  }

  sym{type empty;}

  left
  {
    type            noSlip;

      }
  right
  {
    type            noSlip;
     }
}

